{
    "version": "1.0.0",
    "summary": "Directed graph for representing relationships between UI components.",
    "repository": "https://github.com/flarebyte/bubblegum-graph.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Bubblegum.Graph"
        , "Bubblegum.GraphPaths"
        , "Bubblegum.Node"
        , "Bubblegum.Edge"
        , "Bubblegum.Relations"
        , "Bubblegum.Path"   
        , "Bubblegum.ValueKey"
        , "Bubblegum.ValueDict"
        , "Bubblegum.Irrelevant"       
    ],
    "dependencies": {
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
